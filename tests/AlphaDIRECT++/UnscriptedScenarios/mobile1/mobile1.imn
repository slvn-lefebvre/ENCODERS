node n1 {
    type router
    model PC
    network-config {
	hostname n1
	!
	interface eth0
	 ip address 10.0.0.20/32
	 ipv6 address 2001:0::20/128
	!
    }
    canvas c1
    iconcoords {117.0 373.0}
    labelcoords {117.0 405.0}
    interface-peer {eth0 n6}
    services {DefaultRoute HaggleService}
}

